{
  "extends": "stylelint-config-standard",
  "rules": {
    "at-rule-no-unknown": [
      true,
      { "ignoreAtRules": ["mixin", "extend", "content", "each", "if", "include"] }
    ],
    "media-feature-name-no-unknown": [
      true,
      {
        "ignoreMediaFeatureNames": ["min-device-pixel-ratio"]
      }
    ]
  }
}
